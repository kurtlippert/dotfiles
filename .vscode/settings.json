{
    "workbench.colorTheme": "Default Light+",
    "keyboard.dispatch": "keyCode",
    "vim.easymotion": true,
    "vim.enableNeovim": true,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<C-j>"],
            "after": ["o", "<C-[>"]
        },
        {
            "before": ["<C-k>"],
            "after": ["O", "<C-[>"]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-d>": true,
        "<C-f>": false,
        "<C-s>": false,
        "<C-z>": false
    }
}
